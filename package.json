{
  "name": "collator",
  "description": "Collator gets information, organises it and serves it up when required.",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ocean/collator.git"
  },
  "scripts": {
    "build": "BABEL_ENV=production babel -d dist/ src/",
    "dev": "nodejs-dashboard -- nodemon -r nodejs-dashboard src/bootstrap.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src/ --ignore-path .gitignore",
    "mon": "nodemon src/bootstrap.js",
    "security_check": "snyk test",
    "start": "node dist/app.js",
    "test": "ava"
  },
  "author": "Drew Robinson <drew.robinson@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "8.x"
  },
  "ava": {
    "files": [
      "src/tests/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "boom": "^6.x",
    "catbox": "^9.0.0",
    "catbox-redis": "^3.1.1",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^4.0.0",
    "etagger": "^3.0.1",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-guy-http": "^1.10.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "he": "^1.1.1",
    "inert": "^4.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nock": "^9.0.27",
    "nodejs-dashboard": "^0.4.1",
    "nyc": "^11.3.0",
    "supertest": "^3.0.0"
  }
}
