{
  "name": "collator",
  "description": "Collator gets information, organises it and serves it up when required.",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ocean/collator.git"
  },
  "scripts": {
    "build": "babel -d dist/ src/",
    "dev": "nodejs-dashboard -- nodemon -r nodejs-dashboard src/bootstrap.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src/ --ignore-path .gitignore",
    "security_check": "snyk test",
    "start": "node dist/app.js",
    "test": "ava"
  },
  "author": "Drew Robinson <drew.robinson@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "7.x"
  },
  "ava": {
    "files": [
      "src/tests/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^4.0.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-guy-http": "^1.9.1",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.5.2",
    "inert": "^4.2.1",
    "moment": "^2.18.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "nock": "^9.x",
    "nodejs-dashboard": "^0.4.1",
    "supertest": "^3.0.0"
  }
}
