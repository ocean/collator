{
  "name": "collator",
  "description": "Collator gets information, organises it and serves it up when required.",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/ocean/collator.git"
  },
  "scripts": {
    "build": "babel -d dist/ src/",
    "start": "node dist/app.js",
    "dev": "nodejs-dashboard -- nodemon -r nodejs-dashboard src/bootstrap.js",
    "lint": "eslint src/ --ignore-path .gitignore",
    "security_check": "snyk test",
    "test": "tap --reporter=spec --coverage *.test.js"
  },
  "author": "Drew Robinson <drew.robinson@gmail.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^6.x"
  },
  "ava": {
    "files": [
      "src/tests/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "dotenv": "^4.0.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-guy-http": "^1.9.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.4.1",
    "inert": "^4.2.0",
    "moment": "^2.18.1",
    "twitter": "^1.7.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "locate-path": "^2.0.0",
    "nock": "^9.x",
    "nodejs-dashboard": "^0.4.1",
    "tap": "^10.x"
  }
}
